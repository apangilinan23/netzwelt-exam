@model TerritoryViewModel

<div class="row">
    <div class="mx-auto col-lg-8">
        <div class="row">
            <div class="col-lg-12">
                <h3>Territories</h3>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-12">
                @{
                    foreach (var groupedPlace in Model.Territories)
                    {
                        <ul class="list-group">
                            <li class="list-group-item">
                                @groupedPlace.Parent
                                <ul class="list-group">
                                    @{
                                        foreach (var child in groupedPlace.Children)
                                        {
                                            <li class="list-group-item">
                                                @child
                                            </li>
                                        }
                                    }
                                </ul>
                            </li>
                        </ul>
                    }
                }
                <ul>
                </ul>
            </div>
        </div>
    </div>
</div>
